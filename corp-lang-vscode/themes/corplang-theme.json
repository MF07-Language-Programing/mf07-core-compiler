{
    "name": "CorpLang Theme",
    "type": "dark",
    "colors": {
        "editor.background": "#1a1527",
        "editor.foreground": "#e8e3f3",
        "editor.lineHighlightBackground": "#2d2440",
        "editor.selectionBackground": "#4a3969",
        "editorCursor.foreground": "#a855f7",
        "editorLineNumber.foreground": "#6b46c1",
        "editorLineNumber.activeForeground": "#a855f7",
        "sideBar.background": "#16141f",
        "activityBar.background": "#110e19",
        "statusBar.background": "#6366f1",
        "panel.background": "#1a1527"
    },
    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#8b5fbf"
            }
        },
        {
            "name": "CorpLang Keywords - Control",
            "scope": [
                "keyword.control.corplang"
            ],
            "settings": {
                "foreground": "#a855f7",
                "fontStyle": "bold"
            }
        },
        {
            "name": "CorpLang Keywords - Declaration",
            "scope": [
                "keyword.declaration.corplang"
            ],
            "settings": {
                "foreground": "#8b5cf6",
                "fontStyle": "bold"
            }
        },
        {
            "name": "CorpLang Keywords - Modifier",
            "scope": [
                "keyword.modifier.corplang"
            ],
            "settings": {
                "foreground": "#7c3aed",
                "fontStyle": "italic"
            }
        },
        {
            "name": "CorpLang Keywords - Corporate",
